<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object id="TopLevel" class="tk.Toplevel">
    <property name="background">#ebebeb</property>
    <property name="cursor">arrow</property>
    <property name="maxsize">1000|1000</property>
    <property name="minsize">400|400</property>
    <property name="padx">20</property>
    <property name="pady">20</property>
    <property name="relief">flat</property>
    <child>
      <object id="Frame_2" class="ttk.Frame">
        <property name="height">0</property>
        <property name="width">0</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object id="Frame_7" class="ttk.Frame">
        <property name="height">0</property>
        <property name="width">0</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object id="Frame_1" class="ttk.Frame">
        <property name="borderwidth">0</property>
        <property name="height">0</property>
        <property name="padding">0</property>
        <property name="width">0</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">e</property>
          <rows>
            <row id="0">
              <property name="pad">0</property>
            </row>
          </rows>
          <columns>
            <column id="0">
              <property name="pad">10</property>
            </column>
            <column id="1">
              <property name="pad">10</property>
            </column>
            <column id="2">
              <property name="pad">10</property>
            </column>
          </columns>
        </layout>
        <child>
          <object id="DebugButton" class="ttk.Checkbutton">
            <property name="command">on_log_type_changed</property>
            <property name="takefocus">true</property>
            <property name="text" translatable="yes">Debug</property>
            <property name="variable">boolean:is_show_debug</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="WarningButton" class="ttk.Checkbutton">
            <property name="command">on_log_type_changed</property>
            <property name="text" translatable="yes">Warning</property>
            <property name="variable">string:is_show_warning</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="ErrorButton" class="ttk.Checkbutton">
            <property name="command">on_log_type_changed</property>
            <property name="text" translatable="yes">Error</property>
            <property name="variable">string:is_display_error</property>
            <layout>
              <property name="column">2</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object id="Frame_8" class="ttk.Frame">
        <property name="height">0</property>
        <property name="width">0</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object id="Full Log" class="ttk.Labelframe">
        <property name="height">150</property>
        <property name="text" translatable="yes">Log</property>
        <property name="width">350</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">False</property>
          <property name="row">4</property>
        </layout>
        <child>
          <object id="LogText" class="tk.Text">
            <property name="autoseparators">true</property>
            <property name="blockcursor">false</property>
            <property name="cursor">arrow</property>
            <property name="exportselection">true</property>
            <property name="height">10</property>
            <property name="insertunfocussed">none</property>
            <property name="relief">ridge</property>
            <property name="width">50</property>
            <property name="wrap">word</property>
            <bind sequence="&lt;&lt;ListboxSelect&gt;&gt;" handler="on_listbox_select" add="True" />
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object id="IpLabel" class="ttk.Label">
        <property name="text" translatable="yes">IP: 192.168.218.205</property>
        <property name="textvariable">string:ip_label_text</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">e</property>
        </layout>
      </object>
    </child>
    <child>
      <object id="ClearButton" class="ttk.Button">
        <property name="command">on_click_clear</property>
        <property name="text" translatable="yes">Clear</property>
        <layout>
          <property name="column">0</property>
          <property name="ipadx">0</property>
          <property name="padx">2</property>
          <property name="propagate">False</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object id="scrollbarhelper_1" class="pygubu.builder.widgets.scrollbarhelper">
        <property name="height">150</property>
        <property name="scrolltype">horizontal</property>
        <property name="usemousewheel">true</property>
        <property name="width">350</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">False</property>
          <property name="row">2</property>
        </layout>
        <child>
          <object id="LogListBox" class="tk.Listbox">
            <property name="activestyle">none</property>
            <property name="height">10</property>
            <property name="relief">ridge</property>
            <property name="selectmode">browse</property>
            <property name="takefocus">false</property>
            <property name="width">50</property>
            <bind sequence="&lt;&lt;ListboxSelect&gt;&gt;" handler="on_listbox_select" add="True" />
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">6</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
